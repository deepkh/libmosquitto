SHELL=/bin/sh

BINMOSQUITTO_PUB: ${BINMOSQUITTO_PUB_BIN}
	@echo MAKE BINMOSQUITTO_PUB 

BINMOSQUITTO_PUB_CLEAN: ${BINMOSQUITTO_PUB_BIN_CLEAN}
	@echo MAKE BINMOSQUITTO_PUB_CLEAN 

${BINMOSQUITTO_PUB_BIN}:
	$(CD) ${BINMOSQUITTO_PUB} && $(MAKE) -f Makefile.${BINMOSQUITTO_PUB_NAME} -j8 $@

BINMOSQUITTO_PUB_DEV: 
	$(CD) ${BINMOSQUITTO_PUB} && $(MAKE) -f Makefile.${BINMOSQUITTO_PUB_NAME} -j8 ${BINMOSQUITTO_PUB_BIN}
	@echo MAKE $@ 

${BINMOSQUITTO_PUB_BIN_CLEAN}:
	$(CD) ${BINMOSQUITTO_PUB} && $(MAKE) -f Makefile.${BINMOSQUITTO_PUB_NAME} $@



