	@echo MAKE BINMOSQUITTO_SUB_CLEAN 

${BINMOSQUITTO_SUB_BIN}:
	$(CD) ${BINMOSQUITTO_SUB} && $(MAKE) -f Makefile.${BINMOSQUITTO_SUB_NAME} -j8 $@

